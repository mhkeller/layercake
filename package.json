{
	"name": "layercake",
	"version": "8.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit sync && svelte-package -o dist && publint",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update_template": "sh ./src/scripts/update_template.sh",
		"prebuild": "npm run update_template",
		"deploy": "npm run build && git add -A && git commit -am 'auto-deploy' && git push origin main",
		"test": "mocha --require esm test/*.test.js"
	},
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@rollup/plugin-dsv": "^3.0.2",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.2.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/cookie": "^0.4.1",
		"@types/d3": "^7.4.0",
		"@types/markdown-it": "^12.2.3",
		"@types/node": "^20.4.2",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"cookie": "^0.4.1",
		"d3-collection": "^1.0.7",
		"d3-delaunay": "^6.0.4",
		"d3-force": "^3.0.0",
		"d3-format": "^3.1.0",
		"d3-geo": "^3.1.0",
		"d3-hierarchy": "^3.1.2",
		"d3-quadtree": "^3.0.1",
		"d3-sankey": "^0.12.3",
		"d3-time": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"degit": "^2.8.4",
		"do-not-zip": "^1.0.0",
		"doctrine": "^3.0.0",
		"emoji-regex": "^10.2.1",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.31.1",
		"esm": "^3.2.25",
		"golden-fleece": "^1.0.9",
		"highlight.js": "^11.8.0",
		"html-minifier": "^4.0.0",
		"indian-ocean": "^4.0.3",
		"markdown-it": "^12.3.1",
		"marked": "^4.0.7",
		"mocha": "^10.2.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.12",
		"regl": "^2.1.0",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"svelte2tsx": "^0.6.27",
		"topojson-client": "^3.1.0",
		"tslib": "^2.5.3",
		"typescript": "^5.0.2",
		"underscore": "^1.13.6",
		"vite": "^5.0.0",
		"yootils": "^0.3.1"
	},
	"dependencies": {
		"d3-array": "^3.2.4",
		"d3-color": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	},
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "3 - 4",
		"typescript": "^5.0.2"
	},
	"svelte": "./dist/index.js"
}