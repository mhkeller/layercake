    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.2",
    "tslib": "^2.4.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.3"
