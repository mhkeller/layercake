{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"src/lib/index.js",
		"src/app.html",
		"src/routes/_examples.js",
		"src/routes/_examples_ssr.js",
		"src/routes/_components.js",
		"test/**/*.{js,ts}",
		"src/scripts/**/*.{js,ts}",
		"*.config.{js,ts}"
	],
	"project": [
		"src/**/*.{js,ts,svelte}",
		"test/**/*.{js,ts}",
		"*.config.{js,ts}"
	],
	"ignore": [
		"docs/**",
		"dist/**",
		"test-results/**",
		"playwright-report/**",
		".svelte-kit/**",
		"src/global.d.ts"
	],
	"ignoreDependencies": [
		"@types/d3",
		"degit"
	]
}
