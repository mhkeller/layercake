{
	"name": "layercake",
	"version": "7.3.5",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit sync && svelte-package -o dist && publint",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update_template": "sh ./src/scripts/update_template.sh",
		"prebuild": "npm run update_template",
		"deploy": "npm run build && git add -A && git commit -am 'auto-deploy' && git push origin main",
		"test:mocha": "mocha --require esm test/mocha/*.test.js",
		"test:svelte": "vitest run test/svelte/*.test.js",
		"test": "npm run test:mocha && npm run test:svelte"
	},
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@rollup/plugin-dsv": "^2.0.2",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "1.15.2",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.4",
		"@testing-library/svelte": "^3.2.2",
		"@types/cookie": "^0.4.1",
		"@types/markdown-it": "^12.2.3",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"cookie": "^0.4.1",
		"d3-collection": "^1.0.7",
		"d3-delaunay": "^6.0.2",
		"d3-force": "^3.0.0",
		"d3-format": "^3.1.0",
		"d3-geo": "^3.0.1",
		"d3-hierarchy": "^3.1.1",
		"d3-quadtree": "^3.0.1",
		"d3-sankey": "^0.12.3",
		"d3-time": "^3.0.0",
		"d3-time-format": "^4.1.0",
		"degit": "^2.8.4",
		"do-not-zip": "^1.0.0",
		"doctrine": "^3.0.0",
		"emoji-regex": "^8.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"esm": "^3.2.25",
		"golden-fleece": "^1.0.9",
		"highlight.js": "^11.3.1",
		"html-minifier": "^4.0.0",
		"indian-ocean": "^4.0.2",
		"jsdom": "^21.1.1",
		"markdown-it": "^12.3.0",
		"marked": "^4.0.7",
		"mocha": "^9.2.2",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"publint": "^0.1.10",
		"regl": "^2.1.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.2",
		"svelte2tsx": "^0.6.8",
		"topojson-client": "^3.1.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.2",
		"underscore": "^1.13.1",
		"vite": "^4.2.1",
		"vitest": "^0.30.0",
		"yootils": "^0.3.1"
	},
	"dependencies": {
		"d3-array": "^3.2.1",
		"d3-color": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	},
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^3.54.0",
		"typescript": "^5.0.2"
	},
	"svelte": "./dist/index.js"
}
